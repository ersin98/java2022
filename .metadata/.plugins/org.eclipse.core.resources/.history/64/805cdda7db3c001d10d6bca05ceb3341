package loop;

public class Main {

	public static void main(String[] args) {// For

		for(int i=1;i<10;i+=2){
			System.out.println(i);
		}
		System.out.println("Döngü Bitti");
		int i=1;
		// While
		while(i<10){
		System.out.println(i);
		1+=2;
		}
		System.out.println("While Döngüsü Bitti");
	}

}
