package kodlama.io.Kodlama.io.Devs.dataAccess.abstracts;

import org.springframework.data.jpa.repository.JpaRepository;

import kodlama.io.Kodlama.io.Devs.entities.abstracts.CodingLanguage;

public interface CodingLanguageRepository extends JpaRepository<CodingLanguage, Integer> {

	void save(kodlama.io.Kodlama.io.Devs.entities.concretes.CodingLanguage codingLanguage);

}
// JpaRepository kendi içerisinde hepsini getir şuna göre getir sil gibi
// işlemleri kendi içinde zaten tutmaktadır
// implemet edilmiş gibi bellekte concrete tutar
