package kodlamaIo.entities;

public class Course {
	private int id;
	private String title;
	private String explanation;
	private int price;
	private int progress;

	public Course() {

	}

	public Course(int id, String title, String explanation, int price, int progress) {
		super();
		this.id = id;
		this.title = title;
		this.explanation = explanation;
		this.price = price;
		this.progress = progress;
	}

}
